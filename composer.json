{
    "name": "liip/drupal-testing",
    "description": "A library to integrate PHPUnit with Drupal",
    "keywords": ["testing", "phpunit", "drupal"],
    "type": "library",
    "authors": [
        {
            "name": "Daniel Barsotti",
            "email": "daniel.barsotti@liip.ch"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/liip/LiipDrupalConnectorModule"
        }
    ],
    "require": {
        "php": ">=5.3.0",
    	"monolog/monolog": "dev-master",
        "fabpot/goutte": "dev-master",
        "symfony/console": "dev-master",
        "liip/drupalconnectormodule": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Liip\\Drupal\\Testing": "src"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}": ["liip/drupalconnectormodule"]
        }
    }
}
